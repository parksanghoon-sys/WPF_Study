<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:control="clr-namespace:Custom.Wpf.Global.Controls;assembly=Custom.Wpf"
                    xmlns:units="clr-namespace:wpfSiderBarMenu.UI.Units">

    <Style TargetType="{x:Type units:Navigation1}">
        <Setter Property="Background" Value="Red"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type units:Navigation1}">
                    <Grid Background="Red">
                        <TextBlock Text="{Binding Test}" FontSize="22" Foreground="White"/>
                        <!--<ContentPresenter/>-->
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>