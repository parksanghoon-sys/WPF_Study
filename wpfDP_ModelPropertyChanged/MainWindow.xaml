<Window x:Class="wpfDP_ModelPropertyChanged.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpfDP_ModelPropertyChanged"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Grid>
        <StackPanel>
            <local:FrequencyControl FrequencyValue="{Binding FrequencyModel}"/>
            <Button Content="Test" Command="{Binding OnBtn1ClickCommand}" Width="80" Margin="100"/>
        </StackPanel>
    </Grid>
</Window>
